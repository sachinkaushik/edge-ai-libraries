{
  "name": "chatqna",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json,html,css,scss,yml,yaml,md}\"",
    "prettier:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json,html,css,scss,yml,yaml,md}\" --ignore-unknown",
    "prepare": "husky",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,html,css,scss,yml,yaml,md}": [
      "npm run prettier:write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.26.10",
    "@carbon/react": "^1.67.1",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.8.2",
    "date-fns": "^4.1.0",
    "i18next": "^24.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.2",
    "react-redux": "^9.1.2",
    "reselect": "^5.1.1",
    "rxjs": "^7.8.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@jest/types": "^29.6.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-i18next": "^8.1.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sass": "^1.79.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  }
}
